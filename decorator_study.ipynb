    "# 함수의 데코레이터 사용\n",
    "\n",
    "# 1. 먼저 수행\n",
    "def test(func):\n",
    "    def wrapper():\n",
    "        print(\"start!!!\")\n",
    "        func()\n",
    "        print(\"end!!!\")\n",
    "    return wrapper\n",
    "\n",
    "# 데코레이션을 붙여 함수 생성\n",
    "\n",
    "@test\n",
    "def hello():\n",
    "    print(\"hello\")\n",
    "    \n",
    "    \n",
    "# 함수 호출\n",
    "hello()"
   ]
  }
